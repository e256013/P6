// Descripción: Programa que evalúa expresiones lógicas.

#include <stdio.h>

int main() {
    int a = 5, b = 10;
    if (a > b) {
        printf("\n %d es menor que %d  ", b, a );
    } /*if principal*/ 
    else {
        printf("\n %d es menor que %d ", a, b);
    }//else principal
} //main
